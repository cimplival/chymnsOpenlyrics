<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4" modifiedIn="OpenLP 2.4" modifiedDate="2017-01-14T20:54:13">
  <properties>
    <titles>
      <title>Matega Maitu</title>
    </titles>
    <verseOrder>v1 c1 v2 c1 v3 c1 v4 c1 v5 c1 v6 c1 v7 c1 v8 c1</verseOrder>
    <authors>
      <author>General</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Matega maitu Ngai <br/>twagutegera,<br/>twakuria umamukire <br/>na umarathime.</lines>
    </verse>
    <verse name="c1">
      <lines>Moe, Moee, moe ee,<br/>twakuria umamukire <br/>na umarathime. x2</lines>
    </verse>
    <verse name="v2">
      <lines>Mugate na Divai <br/>twagutegera<br/>...</lines>
    </verse>
    <verse name="v3">
      <lines>Magetha Maitu Ngai <br/>twagutegera<br/>...</lines>
    </verse>
    <verse name="v4">
      <lines>Na mbeca ciitu Ngai <br/>twagutegera<br/>...</lines>
    </verse>
    <verse name="v5">
      <lines>Aciari aitu Ngai <br/>twagutegera<br/>...</lines>
    </verse>
    <verse name="v6">
      <lines>Na ciana ciitu Ngai <br/>twagutegera<br/>...</lines>
    </verse>
    <verse name="v7">
      <lines>Kanitha witu Ngai <br/>twagutegera<br/>...</lines>
    </verse>
    <verse name="v8">
      <lines>Bururi witu Ngai <br/>twagutegera<br/>...</lines>
    </verse>
  </lyrics>
</song>
