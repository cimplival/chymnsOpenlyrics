<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4" modifiedIn="OpenLP 2.4" modifiedDate="2017-01-14T20:53:46">
  <properties>
    <titles>
      <title>Ilinipasa</title>
    </titles>
    <verseOrder>c1 v1 c1 v2 c1 v3 c1 v4 c1</verseOrder>
    <authors>
      <author>Communion</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="c1">
      <lines>Ilinipasa kukutendea <br/>nini zaidi nisikutendee. x2</lines>
    </verse>
    <verse name="v1">
      <lines>Kweli nilikupenda,<br/>kama shamba nzuri <br/>mno la mzabibu.</lines>
    </verse>
    <verse name="v2">
      <lines>Ee Mungu wangu<br/>utuhurumie.</lines>
    </verse>
    <verse name="v3">
      <lines>Wewe umekuwa,<br/>mchungu kabisa kwangu.</lines>
    </verse>
    <verse name="v4">
      <lines>Ulinichoma mkuki ubavuni,<br/>mimi mwokozi wako.</lines>
    </verse>
  </lyrics>
</song>
