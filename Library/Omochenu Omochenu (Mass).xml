<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4" modifiedIn="OpenLP 2.4" modifiedDate="2017-01-14T20:54:43">
  <properties>
    <titles>
      <title>Omochenu Omochenu</title>
    </titles>
    <verseOrder>v1 c1 v2 c1 v3 c1</verseOrder>
    <authors>
      <author>Mass</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Omochenu, omochenu, <br/>Monguru,<br/>Nyasae bwe'chinuko.</lines>
    </verse>
    <verse name="c1">
      <lines>Masoge, masoge, <br/>masoge ase igoro.</lines>
    </verse>
    <verse name="v2">
      <lines>Igoro nanse, chiachoirwe,<br/>No'bwari bwao bwobonene. </lines>
    </verse>
    <verse name="v3">
      <lines>Nare n'egesio oyochire,<br/>As'rieta ri'omonene. x2</lines>
    </verse>
  </lyrics>
</song>
