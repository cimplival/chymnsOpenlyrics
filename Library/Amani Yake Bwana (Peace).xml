  <lyrics>
    <verse name="v1">
      <lines>Amani yake<br/>Bwana isambae (Bwana).</lines>
    </verse>
    <verse name="c1">
      <lines>Mioyoni mwetu <br/>daima milele.</lines>
    </verse>
    <verse name="v2">
      <lines>Upendo wake<br/>Bwana usambae (Bwana).</lines>
    </verse>
    <verse name="v3">
      <lines>Neema yake<br/>Bwana isambae (Bwana).</lines>
    </verse>
    <verse name="v4">
      <lines>Baraka yake<br/>Bwana isambae (Bwana).</lines>
    </verse>
    <verse name="v5">
      <lines>Ushindi wake<br/>Bwana usambae (Bwana).</lines>
    </verse>
  </lyrics>
