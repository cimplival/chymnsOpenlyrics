<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4" modifiedIn="OpenLP 2.4" modifiedDate="2017-01-14T20:54:25">
  <properties>
    <titles>
      <title>Nating'o Muma</title>
    </titles>
    <verseOrder>c1 v1 c1 v2 v3 c1 v4 c1 v5 c1 v5 c1 v6 c1 v7 c1 v8</verseOrder>
    <authors>
      <author>Neno</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="c1">
      <lines>Ting’o muma oo <br/>ting’o muma,<br/>Ting’o muma nalemo. x2</lines>
    </verse>
    <verse name="v1">
      <lines>Nating’o muma nalemo,<br/>Nating’o muma, <br/>nalemo, nating’ang’o.</lines>
    </verse>
    <verse name="v2">
      <lines>Naweyo chode nalemo,<br/>Naweyo chode nalemo <br/>naweyang’o<br/>(Weyo chode weyo chode <br/>(naweyo ang’o)<br/>weyo chode nalemo). x2</lines>
    </verse>
    <verse name="v3">
      <lines>Naweyo kong’o nalemo . . .</lines>
    </verse>
    <verse name="v4">
      <lines>Naweyo magpiny nalemo . . .</lines>
    </verse>
    <verse name="v5">
      <lines>Naweyo njaga nalemo . . .</lines>
    </verse>
    <verse name="v6">
      <lines>Naweyo kwede nalemo . . .</lines>
    </verse>
    <verse name="v7">
      <lines>Naweyo kwalo nalemo . . .</lines>
    </verse>
    <verse name="v8">
      <lines>Naweyo ndawa nalemo . . .</lines>
    </verse>
  </lyrics>
</song>
