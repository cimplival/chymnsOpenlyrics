<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4" modifiedIn="OpenLP 2.4" modifiedDate="2017-01-14T20:55:18">
  <properties>
    <titles>
      <title>Twendeni Wote</title>
    </titles>
    <verseOrder>c1 v1 c1 v2 c1 v3 c1 v4 c1 v5 c1 v6 c1 v7 c1 v8 c1</verseOrder>
    <authors>
      <author>Entrance</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="c1">
      <lines>Twendeni wote,<br/>twendeni wote<br/>tumwabudu Bwana: <br/>kwa furaha. x2</lines>
    </verse>
    <verse name="v1">
      <lines>Bwana ni mtawala,<br/>amevaa utukufu -</lines>
    </verse>
    <verse name="v2">
      <lines>Mbingu zifurahi, <br/>dunia ishangilie -</lines>
    </verse>
    <verse name="v3">
      <lines>Mashuhuda yake <br/>yanastahili imani -</lines>
    </verse>
    <verse name="v4">
      <lines>Anatutawala kwa <br/>haki na uaminifu -</lines>
    </verse>
    <verse name="v5">
      <lines>Tumpigie shangwe <br/>mwamba wa wokovu<br/>wetu -</lines>
    </verse>
    <verse name="v6">
      <lines>Njoni tumsujudu yeye<br/>aliyetuumba -</lines>
    </verse>
    <verse name="v7">
      <lines>Kwa nyimbo za sifa<br/>tujongee uso wake -</lines>
    </verse>
    <verse name="v8">
      <lines>Enyi waadili, <br/>tukuzeni jina lake -</lines>
    </verse>
  </lyrics>
</song>
