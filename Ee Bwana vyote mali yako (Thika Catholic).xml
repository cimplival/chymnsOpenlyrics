<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4" modifiedIn="OpenLP 2.4" modifiedDate="2017-01-14T20:53:15">
  <properties>
    <titles>
      <title>Ee Bwana vyote mali yako</title>
    </titles>
    <verseOrder>v1 c1 v2 c1 v3 c1 v4 c1 v5 c1 v6 c1 v7 c1 v8 c1</verseOrder>
    <authors>
      <author>Thika Catholic</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ee Bwana vyote mali yako (Bwana) —</lines>
    </verse>
    <verse name="c1">
      <lines>Twakupa vyote mali yako.</lines>
    </verse>
    <verse name="v2">
      <lines>Pokea mkate na divai (Bwana) — </lines>
    </verse>
    <verse name="v3">
      <lines>Mazao ya mashamba yetu (Bwana) —</lines>
    </verse>
    <verse name="v4">
      <lines>Pokea nazo nia zetu (Bwana) —</lines>
    </verse>
    <verse name="v5">
      <lines>Pokea nazo nyoyo zetu (Bwana) —</lines>
    </verse>
    <verse name="v6">
      <lines>Na fedha za mifuko yetu (Bwana) —</lines>
    </verse>
    <verse name="v7">
      <lines>Na kazi ya mikono yetu (Bwana) —</lines>
    </verse>
    <verse name="v8">
      <lines>Furaha na uchungu wetu (Bwana) —</lines>
    </verse>
  </lyrics>
</song>
